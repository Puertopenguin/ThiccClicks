{{# each products }}
    <div class="row">
                {{# each this }}
                    <div class="col-sm-6">
                        <div class="thumbnail">
                            <img src="{{ this.imagePath }}"" alt="" class="img-responsive">
                             <div class="caption">
                                 <h3>{{ this.title }}</h3>
                                 <div>
                                 <p class="description">{{ this.description }}</p>
                                 <div class="clearfix">
                                    <div class="price float-left">${{ this.price }}</div>
                                    <a href="#" class="btn btn-success float-right" role="button"> Add to Cart </a>
                                    </div>
                                 </div>
                             </div>
                        </div>
                    </div>
            {{/each}}
        </div>
{{/each}}




